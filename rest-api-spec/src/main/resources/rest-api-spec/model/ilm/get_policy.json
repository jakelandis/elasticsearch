{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^.*$": {
      "type": "object",
      "properties": {
        "version": {
          "type": "integer"
        },
        "modified_date": {
          "type": "string"
        },
        "policy" : {
          "$ref":  "policy.json#/definitions/policy"
        }
      }
    }
  }
}