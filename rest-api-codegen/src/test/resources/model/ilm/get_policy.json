{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^.*$": {
      "type": "object",
      "properties": {
        "version": {
          "type": "number"
        },
        "modified_date": {
          "type": "string"
        }
      }
    }
  }
}